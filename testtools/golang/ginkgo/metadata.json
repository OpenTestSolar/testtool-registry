      "meta": {
        "schemaVersion": 1.0,
        "name": "ginkgo",
        "nameZh": "Ginkgo自动化测试工具",
        "legacyName": "",
        "description": "Ginkgo自动化测试工具",
        "version": "0.2.0",
        "lang": "golang",
        "defaultBaseImage": "golang:1.22",
        "langType": "COMPILED",
        "parameterDefs": [
          {
            "name": "workerCount",
            "value": "并发数",
            "desc": "并发数",
            "default": "0",
            "inputWidget": "number"
          },
          {
            "name": "parseMode",
            "value": "加载用例的模式",
            "desc": "加载用例的模式",
            "default": "ast",
            "inputWidget": "text"
          },
          {
            "name": "showSetup",
            "value": "上报测试套Before/After",
            "desc": "将测试套的before suite以及after suite作为测试用例上报",
            "default": "false",
            "choices": [
              {
                "value": "true",
                "desc": "是"
              },
              {
                "value": "false",
                "desc": "否"
              }
            ],
            "inputWidget": "choices"
          },
          {
            "name": "withLabels",
            "value": "用例中是否包含标签",
            "desc": "用例中是否包含标签",
            "default": "false",
            "choices": [
              {
                "value": "true",
                "desc": "是"
              },
              {
                "value": "false",
                "desc": "否"
              }
            ],
            "inputWidget": "choices"
          },
          {
            "name": "splitBySpace",
            "value": "上报用例名是否以空格分割",
            "desc": "上报用例名是否以空格分割",
            "default": "false",
            "choices": [
              {
                "value": "true",
                "desc": "是"
              },
              {
                "value": "false",
                "desc": "否"
              }
            ],
            "inputWidget": "choices"
          },
          {
            "name": "concurrentBuild",
            "value": "并发编译",
            "desc": "是否并发编译生成二进制可执行文件",
            "default": "false",
            "choices": [
              {
                "value": "true",
                "desc": "是"
              },
              {
                "value": "false",
                "desc": "否"
              }
            ],
            "inputWidget": "choices"
          },
          {
            "name": "compressBinary",
            "value": "是否压缩编译后生成的二进制文件",
            "desc": "是否压缩编译后生成的二进制文件，镜像中需要包含`upx`工具",
            "default": "false",
            "choices": [
              {
                "value": "true",
                "desc": "是"
              },
              {
                "value": "false",
                "desc": "否"
              }
            ],
            "inputWidget": "choices"
          }
        ],
        "homePage": "https://github.com/OpenTestSolar/testtool-golang-ginkgo",
        "versionFile": "https://testsolar-1321258242.cos.ap-guangzhou.myqcloud.com/testtools/golang/ginkgo/metadata.json",
